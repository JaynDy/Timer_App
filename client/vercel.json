{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self'; connect-src 'self' https://timer-app-server.onrender.com; script-src 'self'; style-src 'self' 'unsafe-inline';"
        }
      ]
    }
  ]
}
